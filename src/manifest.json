{
  "name": "Bowery Comp Tool",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "bowery_icon.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "history",
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "sessions",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "*"
  ],
  "icons": {
    "128": "bowery_icon.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}